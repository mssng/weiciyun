body
{margin:0 auto;
 font-size:12px;
 font-family:Verdana;
 line-height:1.5;
}
ul
{
 list-style:none;
}
img
{
 border:0px;
}
a
{
 color:#05a;
 text-decoration:none;
}
a:hover{color:#f00;}

/*定义container的样式*/
#container
{
 width:960px;
 margin:0 auto;
}
/*定义header的样式*/
#header { height:155px;}

 

/*定义main的样式*/
#maincontent { margin-bottom:8px; overflow: auto; zoom:1;}
#main { float:left; width:664px; height:480px; background:#99d9e8;}
#side { float:right; width:296px; height:480px; background:#99d9e8;}
#main_produce{padding-top:38px;padding-left:20px;}
#firsta{padding-top:180px;padding-left:56px;}

#btn{padding-top:50px;padding-left:10px;width:200px;}
.imagekuang{background-image:url(../images/btn.png);width:250px;height:460px;}
#choose{padding-top:10px;padding-left:80px;}

/*定义footer的样式*/
#footer { height:50px;}

/*定义导航栏样式*/
#nav {height:40px; background:#fc8056;}
.nav_main {height:36px;overflow:hidden;}
.nav_main ul li {float:left;font-size:19px;font-weight:bold;margin:0 5px 0 5px;}
.nav_main ul li a {float:left;display:block;height:26px;line-height:35px;color:#fff;padding-left:20px;}
.nav_main ul li a span{display:block;padding-right:20px;}
